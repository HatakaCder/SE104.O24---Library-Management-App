<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:QuanLyThuVien.ViewModel"
                    xmlns:view="clr-namespace:QuanLyThuVien.View">
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReaderUCVM}">
        <view:Reader/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BookVM}">
        <view:Book/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BookBorrowVM}">
        <view:BookBorrow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LibrarianVM}">
        <view:Librarian></view:Librarian>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:Report}">
        <view:Report/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Setting/>
    </DataTemplate>
</ResourceDictionary>